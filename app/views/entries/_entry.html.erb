<div id="<%= dom_id entry %>">
  <%= render partial: "timer", locals: { entry: entry } %>
  <p>
    <strong>Project:</strong>
    <%= entry.project %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= entry.description %>
  </p>

</div>
<turbo-frame id="timer_<%= dom_id entry %>" />
  <%= form_with(model: entry, data: { turbo_frame: "timer_#{dom_id entry}" }) do |f| %>
    <%= f.fields_for :timers do |ff| %>
      <%= ff.hidden_field :end, {value: DateTime.now} %>
    <% end %>
    <%= f.submit %>
  <% end %>
</turbo-frame>
<p>
  <%= link_to "Show this entry", entry %>
</p>
