<p style="color: green"><%= notice %></p>
<% content_for :title, "Entries" %>
<h1 class="text-5xl mb-6">Entries</h1>
<%= turbo_frame_tag "create_entry" do %>
  <%= render partial: "form", locals: { entry: @entry } %>
<% end %>
<%= turbo_frame_tag "entries" do %>
  <div id="entries" class="flex flex-col items-start gap-2">
    <% @entries.each do |entry| %>
      <%= render entry %>
    <% end %>
  </div>
<% end %>
